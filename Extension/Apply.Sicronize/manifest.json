{
  "manifest_version": 2,
  "name": "Aplly.Synchronize",
  "version": "0.1",
  "icons": {
    "128": "Icon.png"    
  },
  "browser_action": {
    "default_popup": "./popup/popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "./scripts/content.js"
      ]
    }
  ],
  "permissions": [
    "activeTab"
  ]
}